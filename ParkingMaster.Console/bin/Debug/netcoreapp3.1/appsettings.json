{
  "app": {
    "environment": "local",
    "version": "v1"
  },
  "rules": [
    {
      "type": "PLATE",
      "regexValidation": "^([1-9][0-9]{4})$",
      "status": true
    },
    {
      "type": "TIME",
      "regexValidation": "^((86400)|(86[0-3][0-9]{2})|(8[0-5][0-9]{3})|([0-7]?[0-9]{1,4}))$",
      "status": true
    },
    {
      "type": "MOVE",
      "regexValidation": "^(0|1)$",
      "status": true
    }
  ],

  "logger": {
    "path": "C:\\Temp\\",
    "name": "management_local"
  }
}